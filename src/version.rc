#include "version.h"

1 VERSIONINFO
	FILEVERSION PROGRAM_VERSION
	PRODUCTVERSION PROGRAM_VERSION
	FILEFLAGSMASK 0x3fL
#ifdef DEBUG
	FILEFLAGS 0x1L
#else
	FILEFLAGS 0x0L
#endif
	FILEOS 0x40004L
	FILETYPE 0x1L
	FILESUBTYPE 0x0L
BEGIN
	BLOCK "StringFileInfo"
	BEGIN
		BLOCK "040904b0"
		BEGIN
			VALUE "CompanyName", "Daniel Starke"
			VALUE "FileDescription", "Snapmaker 2.0 LightBurn Post-Processor"
			VALUE "FileVersion", PROGRAM_VERSION_STR
			VALUE "InternalName", "sm2lbpp"
			VALUE "LegalCopyright", "Copyright (C) 2023"
			VALUE "OriginalFilename", "sm2lbpp.exe"
			VALUE "ProductName", "Snapmaker 2.0 LightBurn Post-Processor"
			VALUE "ProductVersion", PROGRAM_VERSION_STR
		END
	END
	BLOCK "VarFileInfo"
	BEGIN
		VALUE "Translation", 0x409, 1200
	END
END
